<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    app:startDestination="@+id/categoriesFragment">

    <fragment
        android:id="@+id/categoriesFragment"
        android:name="com.example.licentaincercarea1.CategoriesFragment"
        android:label="categoriesFragment"
        tools:layout="@layout/categories_fragment" >
        <action
            android:id="@+id/action_categoriesFragment_to_reteteFragment"
            app:destination="@id/reteteFragment" />
    </fragment>
    <fragment
        android:id="@+id/manifesting"
        android:name="com.example.licentaincercarea1.manifesting"
        android:label="manifesting"
        tools:layout="@layout/manifesting"/>
    <fragment
        android:id="@+id/manifesting2"
        android:name="com.example.licentaincercarea1.FavouriteFragment"
        android:label="manifesting2"
        tools:layout="@layout/favourite_fragment" />
    <fragment
        android:id="@+id/reteteFragment"
        android:name="com.example.licentaincercarea1.ReteteFragment"
        android:label="reteta"
        tools:layout="@layout/reteta" >
        <action
            android:id="@+id/action_reteteFragment_to_categoriesFragment"
            app:destination="@id/categoriesFragment" >
            <argument
                android:name="nume"
                app:argType="string" />
            <argument
                android:name="ingrediente"
                app:argType="string" />
            <argument
                android:name="imagine"
                app:argType="string" />
            <argument
                android:name="pasi"
                app:argType="string" />
        </action>
    </fragment>
</navigation>